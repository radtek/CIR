<?xml version="1.0" encoding="utf-8"?>
<!-- TemplateGroups Schema definition. Used for verification of templategroups.config files. -->
<xs:schema id="templateGroupsDefinition" targetNamespace="http://sd/llblgen/pro/templateGroupsDefinition.xsd" 
		   elementFormDefault="qualified" xmlns="http://sd/llblgen/pro/templateGroupsDefinition.xsd" xmlns:mstns="http://sd/llblgen/pro/templateGroupsDefinition.xsd" 
	xmlns:xs="http://www.w3.org/2001/XMLSchema">
	<xs:element name="templateGroups">
		<xs:complexType>
			<xs:sequence minOccurs="0" maxOccurs="unbounded">
				<xs:element name="templateGroup">
					<xs:complexType>
						<xs:attribute name="name" type="xs:string" use="required" />
						<xs:attribute name="description" type="xs:string" use="required" />
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>