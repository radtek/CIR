debug : $(SOURCES)
	csc /t:library /debug /out:DotNET1.0\bin\SD.LLBLGen.Pro.DQE.SqlServer.NET10.dll /d:TRACE,DEBUG /lib:$(REFDIR) /r:$(REFERENCES) $(SOURCES)

clean:
	del DotNET1.0\bin\SD.LLBLGen.Pro.DQE.SqlServer.NET10.dll